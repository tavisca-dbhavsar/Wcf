<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAddandCreate" />
                <binding name="BasicHttpBinding_IRetrieve" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IAddandCreate" />
                <binding name="WSHttpBinding_IRetrieve" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:50254/EmployeeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAddandCreate"
                contract="ClientService.IAddandCreate" name="BasicHttpBinding_IAddandCreate" />
            <endpoint address="http://localhost:50254/EmployeeService.svc/ws"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IAddandCreate"
                contract="ClientService.IAddandCreate" name="WSHttpBinding_IAddandCreate">
                <identity>
                    <userPrincipalName value="dbhavsar@pune.tavisca.com" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:50254/EmployeeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRetrieve"
                contract="ClientService.IRetrieve" name="BasicHttpBinding_IRetrieve" />
            <endpoint address="http://localhost:50254/EmployeeService.svc/ws"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IRetrieve"
                contract="ClientService.IRetrieve" name="WSHttpBinding_IRetrieve">
                <identity>
                    <userPrincipalName value="dbhavsar@pune.tavisca.com" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>